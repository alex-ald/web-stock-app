{
  "name": "web-stock-project",
  "version": "1.0.0",
  "description": "Web Stock Project",
  "scripts": {
    "test": "cross-env NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- --watch",
    "start": "cross-env NODE_ENV=development node -r \"babel-register\" ./server",
    "start:prod": "cross-env NODE_ENV=production node ./build/server.js",
    "clean": "rmdir /s /q build",
    "build": "webpack -p  --progress --colors --config ./tools/webpack.client.prod.js && webpack -p --config ./tools/webpack.server.prod.js",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate": "sequelize db:migrate --migrations-path ./database/migrations --config ./database/config.json",
    "migrate:undo:prod": "cross-env NODE_ENV=production sequelize db:migrate:undo",
    "migrate:prod": "cross-env NODE_ENV=production sequelize db:migrate --migrations-path ./database/migrations --config ./database/config.json",
    "migrate:create-table": "sequelize migration:create",
    "seed:create": "sequelize seed:create",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "seed:prod": "cross-env NODE_ENV=production sequelize db:seed:all",
    "seed:undo:prod": "cross-env NODE_ENV=production sequelize db:seed:undo:all"
  },
  "repository": {
    "url": ""
  },
  "keywords": [],
  "author": "Alex Aldana",
  "dependencies": {
    "axios": "^0.15.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.9.0",
    "bignumber.js": "^4.0.0",
    "body-parser": "^1.14.2",
    "chalk": "^1.1.3",
    "cross-env": "2.0.1",
    "ejs": "^2.5.6",
    "eslint-config-standard": "^6.0.0",
    "express": "^4.13.4",
    "helmet": "^2.0.0",
    "history": "^4.0.0",
    "json-loader": "^0.5.4",
    "moment": "^2.18.1",
    "morgan": "^1.6.1",
    "mysql": "^2.13.0",
    "node-schedule": "^1.2.1",
    "opn": "^4.0.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redial": "^0.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.30.1",
    "socket.io": "^2.0.1",
    "standard": "^8.0.0",
    "standard-loader": "^5.0.0"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.0.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "assets-webpack-plugin": "^3.3.0",
    "source-map-support": "^0.4.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "engines": {
    "node": "6.2.2"
  }
}
